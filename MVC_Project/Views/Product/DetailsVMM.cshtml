

@{
    var products = TempData["ListOfProducts"] as List<ProductCopy>;
}

<h1>DetailsVMM</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                ProductPrice
            </th>
            <th>
               ProductQantity
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in products) {
        <tr>
            <td>
                    @item.Price
            </td>
            <td>
                @item.Qty
            </td>
           
        </tr>
}
    </tbody>
</table>
